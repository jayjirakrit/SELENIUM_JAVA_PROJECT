<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All test suite for Login Failed" parallel="tests" thread-count="3">
    <parameter name="suite_name" value="Login_Failed_service"/>
    <test verbose="1" preserve-order="true" name="Login service failed for chrome">
        <parameter name="web_browser" value="chrome"/>
        <parameter name="is_headless" value="false"/>
        <classes>
            <class name="th.co.framework.qa.testcases.ui.framework.LoginFailedTestCase"/>
        </classes>
    </test>
    <test verbose="1" preserve-order="true" name="Login service failed for firefox">
        <parameter name="web_browser" value="firefox"/>
        <parameter name="is_headless" value="false"/>
        <classes>
            <class name="th.co.framework.qa.testcases.ui.framework.LoginFailedTestCase"/>
        </classes>
    </test>
    <test verbose="1" preserve-order="true" name="Login service failed for edge">
        <parameter name="web_browser" value="edge"/>
        <parameter name="is_headless" value="false"/>
        <classes>
            <class name="th.co.framework.qa.testcases.ui.framework.LoginFailedTestCase"/>
        </classes>
    </test>
</suite>
