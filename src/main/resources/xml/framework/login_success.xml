<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All test suite for Login Success" parallel="tests" thread-count="3">
    <parameter name="suite_name" value="Login_Success_service"/>
    <test verbose="1" preserve-order="true" name="Login service success for chrome" parallel="false">
        <parameter name="web_browser" value="chrome"/>
        <parameter name="is_headless" value="false"/>
        <classes>
            <class name="th.co.framework.qa.testcases.ui.framework.LoginSuccessTestCase"/>
        </classes>
    </test>
    <test verbose="1" preserve-order="true" name="Login service success for firefox" parallel="false">
        <parameter name="web_browser" value="firefox"/>
        <parameter name="is_headless" value="false"/>
        <classes>
            <class name="th.co.framework.qa.testcases.ui.framework.LoginSuccessTestCase"/>
        </classes>
    </test>
    <test verbose="1" preserve-order="true" name="Login service success for edge" parallel="false">
        <parameter name="web_browser" value="edge"/>
        <parameter name="is_headless" value="false"/>
        <classes>
            <class name="th.co.framework.qa.testcases.ui.framework.LoginSuccessTestCase"/>
        </classes>
    </test>
</suite>
